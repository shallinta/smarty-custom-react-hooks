{
  "name": "smarty-react-hooks",
  "version": "0.2.11",
  "description": "My custom react hooks used in programs.",
  "github": "https://github.com/shallinta/smarty-react-hooks",
  "author": "John Chan",
  "keywords": [
    "react-hook",
    "react",
    "useState",
    "useEffect",
    "rollup"
  ],
  "module": "dist/index.es.js",
  "main": "dist/index.cjs.js",
  "files": [
